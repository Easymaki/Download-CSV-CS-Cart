function fn_ecl_spec_dev_create_download_file()
{
    $dir = fn_get_public_files_path() . "product_data/";
	fn_rm($dir);
	fn_mkdir($dir);
	$file_path = $dir . "cart_prices_chart.csv";
	$file = fopen( $file_path, 'w' );
	if($file) {
		$chart = db_get_array("SELECT low_range, high_range, price FROM ?:cart_price_chart WHERE 1");
		foreach ($chart as $line) {
			fputcsv($file, $line);

		}
		fclose($file);
		return fn_get_rel_dir($file_path);
	}
	return '';
